<?xml version="1.0" encoding="UTF-8"?>
<nifty xmlns="http://nifty-gui.sourceforge.net/nifty-1.3.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://niftygui.sourceforge.net/nifty-1.3.xsd http://nifty-gui.sourceforge.net/nifty-1.3.xsd">
	<screen id="start" controller="blackhorn.MyScreenController">
		<layer childLayout="center">
			<panel width="800px" height="640px" childLayout="center"
				backgroundColor="#0000">
				<image filename="/data/MainMenu.jpg" childLayout="center">
					<panel controller="blackhorn.ElementController" width="300px"
						height="300px">
						<interact onClick="next()" />
					</panel>
				</image>
			</panel>
		</layer>
	</screen>
</nifty>